    "exchange": {
        "name": "binance",
        "key": "",
        "secret": "",
        "ccxt_config": {
            "enableRateLimit": true,
            "futures": true
        },
        "ccxt_async_config": {
            "enableRateLimit": true,
            "futures": true
        },
        "pair_whitelist": [
            "BTC/USDT:USDT",
            "ETH/USDT:USDT",
            "SOL/USDT:USDT",
            "BNB/USDT:USDT",
            "XRP/USDT:USDT",
            "DOGE/USDT:USDT",
            "ADA/USDT:USDT",
            "SHIB/USDT:USDT",
            "AVAX/USDT:USDT",
            "MATIC/USDT:USDT"
        ],
        "pair_blacklist": [
            ".*UP/USDT.*",
            ".*DOWN/USDT.*"
        ]
    },
    "pairlists": [
        {"method": "StaticPairList"}
    ],
    "telegram": {
        "enabled": false,
        "token": "",
